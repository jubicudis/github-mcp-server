{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch stdio server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}",
            "program": "cmd/github-mcp-server/main.go",
            "args": ["stdio"],
            "console": "integratedTerminal"
        },
        {
            "name": "Launch stdio server (read-only)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}",
            "program": "cmd/github-mcp-server/main.go",
            "args": ["stdio", "--read-only"],
            "console": "integratedTerminal"
        },
       
         
    ]
}
